#!/bin/bash
#Generated by JetBrains Toolbox 1.21.9712 at 2021-09-14T00:08:30.081824

declare -a ideargs=()
declare -- wait=""

for o in "$@"; do
  if [[ "$o" = "--wait" || "$o" = "-w" ]]; then
    wait="-W"
    o="--wait"
  fi
  if [[ "$o" =~ " " ]]; then
    ideargs+=("\"$o\"")
  else
    ideargs+=("$o")
  fi
done

open -na "/Users/t0d0r/Library/Application Support/JetBrains/Toolbox/apps/WebStorm/ch-0/203.7148.54/WebStorm.app/Contents/MacOS/webstorm" $wait --args "${ideargs[@]}"